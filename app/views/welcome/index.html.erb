</div>
<div class="modal-body">
    <!-- Begin MailChimp Signup Form -->
    <div id="mc_embed_signup">
      <h4 id="message"></h4>
      <%= form_for @person, url: register_email_path, remote: true, inner_html: { class: "new_person", id: "new_person" } do |f| %>
        <%= f.email_field :email %>
        <%= f.submit "Submit" %>
      <% end %>
    </div>
