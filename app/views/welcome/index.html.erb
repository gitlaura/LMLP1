<div id="myModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="headerlabel">   
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button><center>          
  </div>
  <div class="modal-body"><center>    
    <h3>Like Minded is not quite ready! 
        Please enter your email address and we will let you know when we launch.</h3></br>      
    <div id="mc_embed_signup">      
      <h4 id="message"></h4>        
      <%= form_for @person, url: register_email_path, remote: true, inner_html: { class: "new_person", id: "new_person" } do |f| %>
      <%= f.email_field :email, :placeholder => "Enter email address" %>
      &nbsp 
      <%= f.submit "Submit", :class => "btn" %>
      <% end %>
    </div>  
  </div>
  <div class="modal-footer">
    <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
  </div>
</div>


<div id="myModalTherapist" class="therapistmodal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="headerlabeltherapist">
       <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button><center>
    </div>
    <div class="therapistmodalbody"><center>
      <div id="therapistform">
        <iframe title="Therapist Contact Form" frameborder="0" src="http://likeminded.wufoo.com/forms/therapist-contact-form/">
          <a href="http://likeminded.wufoo.com/forms/therapist-contact-form/" title="HTML Form">Therapist Contact Form</a>
        </iframe>
      </div>
    </div>
  <div class="modal-footer">
    <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
  </div>
</div>

